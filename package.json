{
  "name": "context7-docs-tool",
  "version": "0.1.0",
  "description": "CLI tool for Context7 documentation workflow automation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "search": "tsx src/index.ts search",
    "get-docs": "tsx src/index.ts get-docs",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "context7",
    "documentation",
    "cli",
    "workflow"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@upstash/context7-sdk": "^0.1.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/prompts": "^2.4.9",
    "tsx": "^4.7.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
